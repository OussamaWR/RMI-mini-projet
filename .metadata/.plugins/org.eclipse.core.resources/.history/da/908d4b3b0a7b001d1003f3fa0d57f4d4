package bookRepository;

import java.rmi.Remote;
import java.rmi.RemoteException;

public interface BookRepository extends Remote {
	 public void Connect() throws RemoteException;
	
	 public void AddBook(String name , String edition , String price , String Author ) throws RemoteException;
	 public void UpdateBook(String name , String edition , String price , String Author ) throws RemoteException;
	 public void  GetAll();
	 
	 
	

}
